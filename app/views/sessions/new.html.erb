<h1>Login</h1>

<div>
    <%= form_for(:session, url: login_path) do |f| %>
        <div>    
        <%= f.label :email %>
        <%= f.email_field :email %>
        </div>
        <div>    
        <%= f.label :password %>
        <%= f.password_field :password %>
        </div>
        <div>
        <%= f.submit "Log In" %>
        </div>
    <% end %>
        
</div>
